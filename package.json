{
  "name": "v-we-ui",
  "version": "0.1.0",
  "description": "vue weui",
  "main": "lib/vWeUI.js",
  "types": "src/index.ts",
  "private": false,
  "files": [
    "src",
    "lib",
    "weui"
  ],
  "scripts": {
    "serve": "vue-cli-service serve --port 8888",
    "build": "npm run build:example & npm run build:docs & npm run build:lib",
    "build:example": "vue-cli-service build --mode production --target app --dashboard",
    "build:lib": "vue-cli-service build --mode production --target lib --dashboard --name vWeUI src/index.ts",
    "lint": "eslint --ignore-path .gitignore . --ext ts --ext vue",
    "lint:fix": "eslint --fix --ignore-path .gitignore . --ext ts --ext vue"
  },
  "keywords": [
    "vue",
    "component"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ScioliGuo/vWeUI.git"
  },
  "homepage": "https://scioliguo.github.io/",
  "author": "zhuyankun",
  "license": "MIT",
  "dependencies": {
    "core-js": "^3.6.4",
    "vue": "^3.0.0-beta.14",
    "vue-router": "^4.0.0-alpha.12"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "@vue/cli-plugin-babel": "^4.4.1",
    "@vue/cli-plugin-eslint": "^4.4.1",
    "@vue/cli-plugin-router": "^4.4.1",
    "@vue/cli-plugin-typescript": "^4.4.1",
    "@vue/cli-service": "^4.4.1",
    "@vue/compiler-sfc": "^3.0.0-beta.14",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "eslint": "^7.1.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "eslint-plugin-vue": "^7.0.0-alpha.4",
    "husky": "^4.2.5",
    "less": "^3.0.4",
    "less-loader": "^6.1.0",
    "lint-staged": "^10.2.7",
    "prettier": "^2.0.5",
    "shelljs": "^0.8.4",
    "typescript": "~3.8.3",
    "vue-cli-plugin-vue-next": "^0.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,vue}": "npm run lint:fix"
  }
}
